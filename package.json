{
    "name": "puskeen",
    "version": "1.0.0",
    "description": "тестовый проект лендинга",
    "main": "index.js",
    "scripts": {
        "build": "rm -rf dist && webpack --mode production",
        "dev": "webpack serve",
        "stylelint": "stylelint '**/*{.css,.scss,.js}'",
        "stylelint:fix": "stylelint '**/*{.css,.scss,.js}' --fix",
        "prettier": "npx prettier --write ."
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{html,css,scss,js}": [
            "prettier --write",
            "stylelint --fix",
            "git add"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Michael2M-dot/pushKeen-landing-.git"
    },
    "author": "Michael2M",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/Michael2M-dot/pushKeen-landing-/issues"
    },
    "homepage": "https://github.com/Michael2M-dot/pushKeen-landing-#readme",
    "dependencies": {
        "core-js": "^3.21.1",
        "prettier": "^2.6.1",
        "stylelint": "^14.6.0",
        "stylelint-config-idiomatic-order": "^8.1.0",
        "stylelint-config-prettier": "^9.0.3",
        "stylelint-config-standard": "^25.0.0",
        "stylelint-order": "^5.0.0",
        "stylelint-prettier": "^2.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.17.8",
        "@babel/preset-env": "^7.16.11",
        "babel-loader": "^8.2.4",
        "husky": "^7.0.4",
        "lint-staged": "^12.3.7",
        "stylelint-scss": "^4.2.0",
        "webpack": "^5.70.0",
        "webpack-cli": "^4.9.2",
        "webpack-dev-server": "^4.7.4"
    }
}
